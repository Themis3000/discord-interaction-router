{
  "compilerOptions": {
    "declaration": true,
    "allowSyntheticDefaultImports": true,
    "module": "es2015",
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "sourceMap": true,
    "strict": true,
    "moduleResolution": "node",
    "target": "es6",
    "importHelpers": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "removeComments": true,
    "baseUrl": "./",
    "rootDirs": ["./src/", "./example/"],
    "outDir": "./dist/",
    "paths": {
      "discord-interaction-router": ["./src/index"]
    }
  },
  "files": [
    "src/index.ts"
  ],
  "exclude": [
    "node_modules",
    "dist",
    "example"
  ]
}
